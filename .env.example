# PostgreSQL connection string used by Prisma
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/quartermaster?schema=public"

# Redis connection string for BullMQ job queues
REDIS_URL="redis://localhost:6379"

# Express server settings
PORT=4000

# GitHub Webhook Secret (generate with: openssl rand -hex 32)
# This should match the secret you configure in GitHub webhook settings
WEBHOOK_SECRET=
