{
  "name": "quartermaster",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "server:dev": "nodemon --watch server --ext ts --exec \"ts-node --project tsconfig.server.json server/index.ts\"",
    "dev:docker": "concurrently \"npm run dev -- --host 0.0.0.0 --port 5173\" \"npm run server:dev\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "node dist-server/index.js",
    "dev:server": "ts-node --project tsconfig.server.json server/index.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.16.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "lucide-react": "^0.356.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "prisma": "^5.16.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
